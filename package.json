{
  "name": "dogana",
  "version": "0.0.1",
  "description": "Payload checking library",
  "main": "index.js",
  "scripts": {
    "test": "nyc --require ts-node/register --require source-map-support/register mocha ./test/**/*.test.ts ./test/*.test.ts"
  },
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "**/*.d.ts",
      "**/types.ts"
    ],
    "all": true,
    "sourceMap": true,
    "instrument": true
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "lodash": "4.17.11",
    "lodash.isempty": "4.4.0",
    "lodash.isnil": "4.0.0",
    "lodash.isnull": "3.0.0",
    "moment": "2.29.2",
    "validator": "10.11.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.7",
    "@types/lodash.isempty": "4.4.6",
    "@types/lodash.isnil": "4.0.6",
    "@types/lodash.isnull": "3.0.6",
    "@types/mocha": "5.2.6",
    "@types/node": "12.0.0",
    "chai": "4.2.0",
    "eslint": "5.16.0",
    "mocha": "6.1.4",
    "node": "12.1.0",
    "nyc": "14.1.0",
    "ts-node": "8.1.0",
    "typescript": "3.4.5",
    "typescript-eslint-parser": "22.0.0"
  }
